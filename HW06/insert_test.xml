<test-suite>
    <test-case>
        <name>insert(15, 3, [2, 9, 6, 7, 8, 4, 10, 11, 3, 14])</name>
        <has-max-executions>1</has-max-executions>
        <max-executions>1000000</max-executions>
        <randomize>1</randomize>
        <input>
            <test-array>
                <address>ARRAY</address>
                <value>2, 9, 6, 7, 8, 4, 10, 11, 3, 14</value>
            </test-array>
            <test-value>
                <address>LENGTH</address>
                <value>10</value>
            </test-value>
            <test-value>
                <address>NUMBER</address>
                <value>15</value>
            </test-value>
            <test-value>
                <address>INDEX</address>
                <value>3</value>
            </test-value>
        </input>
        <output>
            <test-array points="5">
                <address>ARRAY</address>
                <value>2, 9, 6, 15, 7, 8, 4, 10, 11, 3, 14</value>
            </test-array>
        </output>
    </test-case>
